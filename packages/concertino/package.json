    "semver": "7.6.3"
  },
  "devDependencies": {
    "@accordproject/concerto-codegen": "^3.22.0",
    "@accordproject/concerto-cto": "3.22.0",
    "@accordproject/concerto-util": "^3.22.0",
    "@types/semver": "7.5.8",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "eslint": "8.57.1",
    "npm-run-all": "4.1.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
