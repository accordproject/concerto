{
  "name": "@accordproject/concerto-core",
  "version": "4.0.0-alpha.2",
  "description": "Core Implementation for the Concerto Modeling Language",
  "homepage": "https://github.com/accordproject/concerto",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "main": "dist/index.js",
  "browser": "dist/concerto-core.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm-run-all clean",
    "prepublishOnly": "npm run webpack",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "test": "cross-env TZ=UTC nyc mocha -r ts-node/register --recursive -t 10000",
    "test:watch": "cross-env TZ=UTC nyc mocha -r ts-node/register --watch --recursive -t 10000",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "npm run webpack",
    "webpack": "webpack --config webpack.config.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/concerto.git",
    "directory": "packages/concerto-cto"
  },
  "keywords": [
    "schema",
    "data model",
    "verification"
  ],
  "author": "accordproject.org",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "7.26.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.18",
    "@types/semver": "7.3.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "acorn": "8.14.0",
    "acorn-walk": "8.3.4",
    "babel-loader": "9.2.1",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-things": "0.2.0",
    "commander": "2.20.0",
    "cross-env": "7.0.3",
    "doctrine": "3.0.0",
    "eslint": "8.57.1",
    "fs-extra": "11.2.0",
    "jsdoc": "4.0.4",
    "klaw": "4.1.0",
    "license-check-and-add": "2.3.6",
    "mocha": "10.8.2",
    "node-polyfill-webpack-plugin": "4.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "17.1.0",
    "process": "0.11.10",
    "rimraf": "3.0.2",
    "sinon": "19.0.2",
    "sinon-chai": "3.7.0",
    "tmp-promise": "3.0.3",
    "ts-jest": "28.0.4",
    "ts-node": "10.9.1",
    "typescript": "5.7.2",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "xregexp": "5.1.1",
    "yargs": "17.7.2"
  },
  "dependencies": {
    "@accordproject/concerto-codegen": "^3.16.2",
    "@accordproject/concerto-cto": "^4.0.0-alpha.2",
    "@accordproject/concerto-metamodel": "^4.0.0-alpha.0",
    "@accordproject/concerto-util": "^4.0.0-alpha.2",
    "dayjs": "1.11.13",
    "debug": "4.3.7",
    "lorem-ipsum": "2.0.8",
    "randexp": "0.5.3",
    "rfdc": "1.4.1",
    "semver": "7.6.3",
    "urijs": "1.19.11",
    "uuid": "11.0.3",
    "yaml": "2.8.0"
  },
  "browserslist": "> 0.25%, not dead",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "produce-source-map": "true",
    "reporter": [
      "lcov",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "exclude": [],
    "all": true,
    "check-coverage": true,
    "statements": 99,
    "branches": 94.8,
    "functions": 99,
    "lines": 99
