{
  "devDependencies": {
    "@accordproject/concerto-metamodel": "^4.0.0-alpha.0",
    "@types/jest": "29.5.14",
    "@types/path-browserify": "^1.0.3",
    "colors": "1.4.0",
    "coveralls-next": "^5.0.0",
    "dayjs": "1.11.13",
    "eslint": "8.57.1",
    "glob": "11.1.0",
    "jsdoc": "^4.0.2",
    "license-check-and-add": "4.0.5",
    "lockfile-lint": "4.14.1",
    "nyc": "17.1.0",
    "semver": "7.6.3",
    "syncpack": "14.0.0-alpha.32"
  },
"overrides": {
    "concerto-metamodel": "$@accordproject/concerto-metamodel",
    "@accordproject/concerto-metamodel": "$@accordproject/concerto-metamodel"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "workspaces": [
    "packages/concerto-analysis",
    "packages/concerto-core",
    "packages/concertino",
    "packages/concerto-cto",
    "packages/concerto-util",
    "packages/concerto-vocabulary",
    "packages/concerto-linter",
    "packages/concerto-linter/default-ruleset"
  ],
  "name": "concerto",
  "description": "Monorepo for concerto packages",
  "version": "4.0.0-alpha.2",
  "private": true,
  "scripts": {
    "coverage": "node ./scripts/coverage.js \"packages/concerto-*\" && nyc report -t coverage --cwd . --report-dir coverage --reporter=lcov && cat ./coverage/lcov.info",
    "pretest": "npm run licchk",
    "test": "npm run test --workspaces --if-present",
    "licchk": "license-check-and-add check -f license-config.json",
    "build": "npm run build:ordered",
    "build:ordered": "npm run build:level0 && npm run build:level1 && npm run build:level2",
    "build:level0": "npm run build -w packages/concerto-util && npm run build -w packages/concerto-linter",
    "build:level1": "npm run build -w packages/concerto-cto && npm run build -w packages/concerto-core",
    "build:level2": "npm run build -w packages/concerto-analysis -w packages/concerto-vocabulary -w packages/concertino",
    "build:parallel": "npm run build --workspaces --if-present",
    "lint:deps": "syncpack lint",
    "lint:deps:fix": "syncpack fix",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --allowed-hosts npm --validate-https"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/concerto"
  }
}